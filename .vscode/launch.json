    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Phantom",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}\\cmd\\phantom",
            "env": {},
            "args": [
                "-broadcast_listen", 
                "-max_connections=20",
                "-coin_conf=${workspaceRoot}\\config.txt",
                "-masternode_conf=${workspaceRoot}\\masternodes.txt"]
        },
        {
            "name": "Launch Coinconf",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}\\cmd\\coinconf",
            "env": {},
            "args": [
                "-coin_name=alqo",
                "-git_hub=https://github.com/ALQOCRYPTO/ALQO"]
        }
    ]
}